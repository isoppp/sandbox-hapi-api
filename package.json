    "joi-to-json-schema": "^4.0.1",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.16.5",
    "knex-migrate": "^1.6.1",
    "md5": "^2.2.1",
    "mysql": "^2.16.0",
    "objection": "^1.2.0"
  },
  "devDependencies": {
    "lab": "^18.0.2",
    "nodemon": "^1.18.1"
  }
